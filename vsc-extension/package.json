{
  "name": "validator-seeder",
  "displayName": "validator-seeder",
  "description": "VS Code extension for CSV validation and database seeding",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:validator-seeder.validateCsv"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "validator-seeder.validateCsv",
        "title": "Validate CSV"
      },
      {
        "command": "validator-seeder.setPrismaSchemaPath",
        "title": "Set Prisma Schema Path"
    }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "validator-seeder.validateCsv",
          "when": "editorTextFocus && editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prisma": "^5.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1"
  }
}
